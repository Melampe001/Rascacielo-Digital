# Environment Configuration

# Application
NODE_ENV=development
PORT=3000

# Build Configuration
BUILD_OPTIMIZE=true
BUILD_OUTPUT_DIR=./dist

# Security Configuration
SECURITY_LEVEL=moderate
SECURITY_FAIL_ON_HIGH=true

# Logging
LOG_LEVEL=info

# CI/CD
CI=false
GITHUB_ACTIONS=false

# Treesit Cloud Configuration
TREESIT_API_URL=https://api.treesit.cloud
TREESIT_API_KEY=your_api_key_here
TREESIT_API_SECRET=your_api_secret_here
TREESIT_REGION=us-east-1
TREESIT_ENV=production
TREESIT_INSTANCE_TYPE=t3.medium
TREESIT_AUTOSCALING=true
TREESIT_MIN_INSTANCES=1
TREESIT_MAX_INSTANCES=10

# Authentication
JWT_SECRET=your_jwt_secret_here
PASSWORD_HASH_ITERATIONS=10000

# Database (if needed)
# DATABASE_URL=postgres://user:pass@localhost:5432/dbname

# API Keys (if needed)
# API_KEY=your_api_key_here
