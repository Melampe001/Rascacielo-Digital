{
  "name": "rascacielo-digital",
  "version": "1.1.0",
  "description": "Sistema modular con agentes especializados, CI/CD automatizado y arquitectura de microservicios",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "build": "node build.js",
    "clean": "rm -rf dist/ coverage/",
    "cleanup": "bash scripts/cleanup.sh",
    "audit": "bash scripts/audit-dependencies.sh",
    "lint-and-format": "bash scripts/lint-and-format.sh",
    "check": "npm run lint && npm run format:check && npm test",
    "security": "node agents/security-agent.js",
    "deploy": "node scripts/treesit-cli.js deploy",
    "deploy:status": "node scripts/treesit-cli.js status",
    "deploy:rollback": "node scripts/treesit-cli.js rollback",
    "deploy:logs": "node scripts/treesit-cli.js logs",
    "deploy:health": "node scripts/treesit-cli.js health",
    "orchestrate:full": "node agents/orchestrator-agent.js --mode=full",
    "orchestrate:fast": "node agents/orchestrator-agent.js --mode=fast",
    "orchestrate:parallel": "node agents/orchestrator-agent.js --mode=parallel",
    "agents:all": "npm run orchestrate:full",
    "agents:build": "node agents/build-agent.js",
    "agents:security": "node agents/security-agent.js",
    "agents:deploy": "node agents/deploy-agent.js",
    "validate": "npm run lint && npm run format:check && npm test",
    "prepare": "node -e \"try{require('husky').install()}catch(e){}\"",
    "precommit": "npm run validate",
    "setup": "npm install && npm run config:check",
    "config:check": "node scripts/config-check.js",
    "health-check": "node scripts/health-check.js",
    "vercel-build": "echo 'Build completed'"
  },
  "keywords": [
    "agents",
    "ai",
    "automation",
    "ci-cd",
    "microservices",
    "orchestration",
    "devops",
    "modular-architecture"
  ],
  "author": "Melampe001",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Melampe001/Rascacielo-Digital.git"
  },
  "bugs": {
    "url": "https://github.com/Melampe001/Rascacielo-Digital/issues"
  },
  "homepage": "https://github.com/Melampe001/Rascacielo-Digital#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "husky": "^8.0.3",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "winston": "^3.17.0",
    "axios": "^1.7.9",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.3",
    "redis": "^4.7.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.5.0",
    "yaml": "^2.6.1"
  }
}
