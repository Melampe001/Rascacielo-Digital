{
  "name": "rascacielos-digital",
  "version": "1.0.0",
  "description": "Sistema modular con agentes especializados y CI/CD",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "build": "node build.js",
    "clean": "rm -rf dist/ coverage/",
    "security": "node agents/security-agent.js",
    "deploy": "node scripts/treesit-cli.js deploy",
    "deploy:status": "node scripts/treesit-cli.js status",
    "deploy:rollback": "node scripts/treesit-cli.js rollback",
    "deploy:logs": "node scripts/treesit-cli.js logs",
    "deploy:health": "node scripts/treesit-cli.js health",
    "validate": "npm run lint && npm run format:check && npm test",
    "prepare": "node -e \"try{require('husky').install()}catch(e){}\"",
    "precommit": "npm run validate",
    "setup": "npm install && npm run config:check",
    "config:check": "node scripts/config-check.js",
    "health-check": "node scripts/health-check.js",
    "imperial:install": "node scripts/imperial-install.js --full",
    "imperial:install:minimal": "node scripts/imperial-install.js --minimal",
    "imperial:install:custom": "node scripts/imperial-install.js --custom",
    "imperial:verify": "node scripts/verify-installation.js",
    "supreme:orchestrate": "node agents/supreme/supreme-orchestrator-agent.js orchestrate",
    "supreme:analyze": "node agents/supreme/supreme-orchestrator-agent.js info",
    "supreme:dashboard": "node agents/supreme/supreme-orchestrator-agent.js dashboard",
    "guardian:scan": "node agents/supreme/imperial-dependency-guardian-agent.js --scan",
    "guardian:update": "node agents/supreme/imperial-dependency-guardian-agent.js --update",
    "guardian:analyze": "node agents/supreme/imperial-dependency-guardian-agent.js --analyze",
    "guardian:licenses": "node agents/supreme/imperial-dependency-guardian-agent.js --licenses",
    "elite:validate": "node agents/supreme/elite-code-quality-agent.js --validate",
    "elite:fix": "node agents/supreme/elite-code-quality-agent.js --fix",
    "elite:fix:aggressive": "node agents/supreme/elite-code-quality-agent.js --fix --aggressive",
    "elite:complexity": "node agents/supreme/elite-code-quality-agent.js --complexity",
    "elite:badge": "node agents/supreme/elite-code-quality-agent.js --badge",
    "imperial:docs": "node agents/supreme/imperial-documentation-generator-agent.js --generate",
    "imperial:docs:html": "node agents/supreme/imperial-documentation-generator-agent.js --html",
    "imperial:docs:pdf": "node agents/supreme/imperial-documentation-generator-agent.js --pdf",
    "imperial:changelog": "node agents/supreme/imperial-documentation-generator-agent.js --changelog"
  },
  "keywords": [
    "modular",
    "architecture",
    "agents",
    "ci-cd",
    "automation",
    "devops",
    "treesit-cloud"
  ],
  "author": "Melampe001",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Melampe001/Rascacielo-Digital.git"
  },
  "bugs": {
    "url": "https://github.com/Melampe001/Rascacielo-Digital/issues"
  },
  "homepage": "https://github.com/Melampe001/Rascacielo-Digital#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "husky": "^8.0.3",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0"
  },
  "dependencies": {}
}
