{
  "name": "rascacielos-digital",
  "version": "1.0.0",
  "description": "Sistema de desarrollo arquitectÃ³nico modular con agentes especializados y CI/CD gates",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "build": "node build-idempotent.js",
    "build:force": "node build-idempotent.js --force",
    "build:old": "node agents/build-agent.js",
    "watch": "node watch-build.js",
    "watch:build": "node watch-build.js",
    "security": "node agents/security-agent.js",
    "clean": "rm -rf dist/ coverage/ .build-cache/",
    "setup": "npm install && npm run config:check",
    "config:check": "node scripts/config-check.js",
    "health-check": "node scripts/health-check.js",
    "precommit": "node build-idempotent.js && npm run lint && npm test",
    "prepare": "husky install || true"
  },
  "keywords": [
    "modular",
    "architecture",
    "agents",
    "ci-cd",
    "automation",
    "devops"
  ],
  "author": "Melampe001",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Melampe001/rascacielos-digital.git"
  },
  "bugs": {
    "url": "https://github.com/Melampe001/rascacielos-digital/issues"
  },
  "homepage": "https://github.com/Melampe001/rascacielos-digital#readme",
  "devDependencies": {
    "chokidar": "^3.5.3",
    "eslint": "^8.50.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  },
  "dependencies": {},
